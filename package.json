    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "sass-lint": "^1.13.1",
    "typescript-plugin-css-modules": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "typings": "./@types/index.d.ts"
